<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="@dimen/dis_half" android:color="@color/color_b10" />
            <corners android:radius="@dimen/dis_4" />
            <solid android:color="@color/color_f5" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color_b2" />
            <stroke android:width="@dimen/dis_half" android:color="@color/color_b10" />
            <corners android:radius="@dimen/dis_4" />
        </shape>

    </item>


</selector>